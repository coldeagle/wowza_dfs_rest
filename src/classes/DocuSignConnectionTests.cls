            HttpResponse res = new HttpResponse();
            res.setStatusCode(200);
            res.setStatus('OK');
            if (req.getEndPoint().contains('/templates/')) {
            	res.setBody(envelopeTemplateDefinition);
            } else if (req.getEndPoint().contains('/templates')) {
            	res.setBody(envelopeTemplates);
            } else if (req.getEndPoint().contains('/envelopes/') && req.getEndPoint().contains('/custom_fields')) {
            	res.setBody(envCustomFields);
            } else if (req.getEndPoint().contains('/envelopes/')) {
            	res.setBody(envelopeResponse);
            } else if (req.getEndPoint().contains('/envelopes')) {
            	res.setBody(envelopeCreateReponse);
            }
            else {
            	res.setStatusCode(400);
            	res.setStatus('Bad Request');
            }

            return res;
		}
	}
	
