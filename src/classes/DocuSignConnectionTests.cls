	@isTest static void getAllTemplatesTest() {
		// Implement test code
		Test.setMock(HttpCalloutMock.class, new MockAPIResponse());
		DocuSignConnection dsc = new DocuSignConnection();
		Test.startTest();
		HttpResponse res = dsc.getAllTemplates();
		Test.stopTest();
		System.assertEquals(200, res.getStatusCode());
		DocuSignType.EnvelopeTemplates templates = (DocuSignType.EnvelopeTemplates) JSON.deserialize(res.getBody(), DocuSignType.EnvelopeTemplates.class);
